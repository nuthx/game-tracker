{
  "name": "game-tracker",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "ncu:check": "ncu",
    "ncu:update": "ncu -u",
    "deltag": "git tag -d 1.1.111111 && git push origin :refs/tags/1.1.111111",
    "db:push": "prisma generate && prisma db push --force-reset && prisma studio",
    "db:migrate": "prisma migrate dev",
    "docker:build": "docker build -t game-tracker:$(npm pkg get version | tr -d '\"') -t game-tracker:latest .",
    "docker:run": "docker run -p 4200:4200 -e JWT_SECRET=123456 game-tracker:latest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "jose": "^6.0.10",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "node-schedule": "^2.1.1",
    "prisma": "^6.7.0",
    "psn-api": "^2.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-i18next": "^15.5.1",
    "sonner": "^2.0.3",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@stylistic/eslint-plugin-plus": "^4.2.0",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.8"
  }
